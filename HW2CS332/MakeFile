# Sample Makefile to compile C programs

CC = gcc
CFLAGS = -Wall -g  # Replace -g with -O when not debugging

DEPS    = jlplayer_HMW02.c Makefile
OBJS    = jlplayer_HMW02.o

EXECS   = jlplayer_HMW02

all: $(EXECS)

%.o: %.c $(DEPS)
	$(CC) $(CFLAGS) -c -o $@ $<

$(EXECS): $(OBJS)
	$(CC) $(CFLAGS) -o $@ $^

run: $(EXECS)
	./$(EXECS) 

clean:
	/bin/rm -i *.o $(EXECS)